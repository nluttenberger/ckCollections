  <fr:recipeName>Ente mit Pak Choy aus dem Wok</fr:recipeName>
  <fr:recipeKeywords />
  <fr:recipeIntro />
  <fr:recipeIngredients>
    <fr:igdtList>
      <fr:igdtListName />
      <fr:igdtListLine>
        <fr:igdtQuantity>1 EL</fr:igdtQuantity>
        <fr:igdtName ref="zitronengras">Zitronengras, frisches, fein gehackt</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 EL</fr:igdtQuantity>
        <fr:igdtName ref="minze">Minze, frische, fein gehackt</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2</fr:igdtQuantity>
        <fr:igdtName ref="knoblauch">Knoblauchzehe(n), gehackt</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 TL</fr:igdtQuantity>
        <fr:igdtName ref="szechuanpfeffer">Pfeffer, (Szechuanpfeffer)</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 TL</fr:igdtQuantity>
        <fr:igdtName ref="limette">Limettenschale, abgeriebene</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>&#189; TL</fr:igdtQuantity>
        <fr:igdtName ref="paprikapulver">Paprikapulver, scharf</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 TL</fr:igdtQuantity>
        <fr:igdtName ref="st&#228;rke">Maisst&#228;rke</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>600 g</fr:igdtQuantity>
        <fr:igdtName ref="ente">Entenbrust, ohne Haut, in feinen Streifen</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 EL</fr:igdtQuantity>
        <fr:igdtName ref="erdnuss&#246;l">&#214;l, (Erdnuss&#246;l)</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>15 g</fr:igdtQuantity>
        <fr:igdtName ref="sprossen">Sojasprossen</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>60 ml</fr:igdtQuantity>
        <fr:igdtName ref="chilischote">Chilisauce, s&#252;&#223;e</fr:igdtName>
      </fr:igdtListLine>
