  <fr:recipeName>Mangold und ger&#246;stete Paprika-Frittata</fr:recipeName>
  <fr:recipeKeywords />
  <fr:recipeIntro />
  <fr:recipeIngredients>
    <fr:igdtList>
      <fr:igdtListName />
      <fr:igdtListLine>
        <fr:igdtQuantity>4 TL</fr:igdtQuantity>
        <fr:igdtName ref="oliven&#246;l">Oliven&#246;l</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 m.-gro&#223;e</fr:igdtQuantity>
        <fr:igdtName ref="zwiebel">Zwiebel(n), gehackt</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 Zehe/n</fr:igdtQuantity>
        <fr:igdtName ref="knoblauch">Knoblauch</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>&#189; Bund</fr:igdtQuantity>
        <fr:igdtName ref="mangold">Mangold, frisch (ca. 225g)</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>140 g</fr:igdtQuantity>
        <fr:igdtName ref="paprika">Paprika, ger&#246;stete aus dem Glas, in Wasser</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>4 gro&#223;e</fr:igdtQuantity>
        <fr:igdtName ref="ei">Ei(er)</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 gro&#223;e</fr:igdtQuantity>
