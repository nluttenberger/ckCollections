        <fr:igdtQuantity>3 EL</fr:igdtQuantity>
        <fr:igdtName ref="">&#214;l</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 Kopf</fr:igdtQuantity>
        <fr:igdtName ref="mangold">Mangold, ca. 400 g</fr:igdtName>
