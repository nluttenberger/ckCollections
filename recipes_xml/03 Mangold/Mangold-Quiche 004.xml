    <fr:igdtList>
      <fr:igdtListName />
      <fr:igdtListLine>
        <fr:igdtQuantity>100 g</fr:igdtQuantity>
        <fr:igdtName ref="quark">Quark (Magerquark)</fr:igdtName>
      </fr:igdtListLine>
