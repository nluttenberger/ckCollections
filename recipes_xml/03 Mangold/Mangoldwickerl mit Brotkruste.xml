      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2</fr:igdtQuantity>
        <fr:igdtName ref="paprika">Paprikaschote(n)</fr:igdtName>
      </fr:igdtListLine>
