  <fr:recipeName>Mangoldkehlchens Schokoversuchung</fr:recipeName>
  <fr:recipeKeywords />
  <fr:recipeIntro />
  <fr:recipeIngredients>
    <fr:igdtList>
      <fr:igdtListName />
      <fr:igdtListLine>
        <fr:igdtQuantity>8 EL</fr:igdtQuantity>
        <fr:igdtName ref="weizenmehl">Mehl</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>8 EL</fr:igdtQuantity>
        <fr:igdtName ref="zucker">Zucker</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 Msp.</fr:igdtQuantity>
        <fr:igdtName ref="backpulver">Backpulver</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 EL</fr:igdtQuantity>
        <fr:igdtName ref="nutella">Nutella</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 EL</fr:igdtQuantity>
        <fr:igdtName ref="kakao">Kakaopulver</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 kl. Flasche/n</fr:igdtQuantity>
        <fr:igdtName ref="rum">Rumaroma</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 Pck.</fr:igdtQuantity>
        <fr:igdtName ref="kuvert&#252;re">Kuvert&#252;re, geschmolzen</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>2 gro&#223;e</fr:igdtQuantity>
        <fr:igdtName ref="ei">Ei(er)</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>6 EL</fr:igdtQuantity>
