      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 m.-gro&#223;e</fr:igdtQuantity>
        <fr:igdtName ref="zwiebel">Zwiebel(n), rote</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1</fr:igdtQuantity>
