  <fr:recipeName>Mangold-Paprika-Feta-Tarte mit Strudelteig</fr:recipeName>
  <fr:recipeKeywords />
  <fr:recipeIntro />
  <fr:recipeIngredients>
    <fr:igdtList>
      <fr:igdtListName />
      <fr:igdtListLine>
        <fr:igdtQuantity>1 Pck.</fr:igdtQuantity>
        <fr:igdtName ref="teig">Strudelteig (Dinkelstrudelteig), 2 Bl&#228;tter</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>300 g</fr:igdtQuantity>
        <fr:igdtName ref="mangold">Mangold, frischer, gewaschener und in Streifen geschnittener</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 kleine</fr:igdtQuantity>
        <fr:igdtName ref="paprika">Paprikaschote(n), rote, gewaschene und in St&#252;cke geschnittene</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 kleine</fr:igdtQuantity>
        <fr:igdtName ref="zwiebel">Zwiebel(n), gesch&#228;lte und fein gehackte</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 Zehe/n</fr:igdtQuantity>
        <fr:igdtName ref="knoblauch">Knoblauch, gesch&#228;lte und fein gehackte</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1 Handvoll</fr:igdtQuantity>
        <fr:igdtName ref="liebst&#246;ckel">Kr&#228;uter, frische, z. B. Salbei, Thymian, Basilikum, Orgeano, Liebst&#246;ckel oder Petersilie, fein gehackte</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>1</fr:igdtQuantity>
        <fr:igdtName ref="ei">Ei(er)</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>150 ml</fr:igdtQuantity>
        <fr:igdtName ref="milch">Milch</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>n. B.</fr:igdtQuantity>
        <fr:igdtName ref="schafsk&#228;se">Feta-K&#228;se</fr:igdtName>
      </fr:igdtListLine>
      <fr:igdtListLine>
        <fr:igdtQuantity>n. B.</fr:igdtQuantity>
        <fr:igdtName ref="pfeffer">Salz und Pfeffer</fr:igdtName>
      </fr:igdtListLine>
